{
    "routes": [
        {
            "src": "/",
            "dest": "/api/mainpage.js"
        },
        {
            "src": "/students/gpa",
            "dest": "/api/gpa.js"
        },
        {
            "src": "/students/info",
            "dest": "/api/info.js"
        },
        {
            "src": "/students/schedule",
            "dest": "/api/schedule.js"
        },
        {
            "src": "/students/currentclasses",
            "dest": "/api/currentclasses.js"
        },
        {
            "src": "/predictedGPA",
            "dest": "/api/predictedGPA.js",
            "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS", "PATCH"],
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Headers": "Origin, X-Requested-With, Content-Type, Accept",
                "Access-Control-Allow-Credentials": "true"
            },
            "continue": true
        },
        {
            "src": "/satdates",
            "dest": "/api/satdates.js"
        }
    ]
}