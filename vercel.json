{
    "routes": [
        {
            "src": "/",
            "dest": "/api/mainpage.js"
        },
        {
            "src": "/students/gpa",
            "dest": "/api/gpa.js"
        },
        {
            "src": "/students/info",
            "dest": "/api/info.js"
        },
        {
            "src": "/students/schedule",
            "dest": "/api/schedule.js"
        },
        {
            "src": "/students/currentclasses",
            "dest": "/api/currentclasses.js"
        },
        {
            "src": "/predictedGPA",
            "dest": "/api/predictedGPA.js",
            "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS", "PATCH"]
        },
        {
            "src": "/satdates",
            "dest": "/api/satdates.js"
        }
    ],
    "headers": [
        {
          "source": "/.*",
          "headers": [
            {
              "key": "Access-Control-Allow-Origin",
              "value": "*"
            },
            {
              "key": "Access-Control-Allow-Headers",
              "value": "X-Requested-With, Access-Control-Allow-Origin, X-HTTP-Method-Override, Content-Type, Authorization, Accept"
            },
            {
              "key": "Access-Control-Allow-Credentials",
              "value": "true"
            }
          ]
        }
      ]
}